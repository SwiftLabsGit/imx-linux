

/dts-v1/;

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include "imx6ul.dtsi"

/ {
  model = "Freescale i.MX6 UltraLite 14x14 EVK Board";
  compatible = "fsl,imx6ul-14x14-evk", "fsl,imx6ul";

  memory {
      reg = <0x80000000 0x20000000>;
  };

  reserved-memory {
      #address-cells = <1>;
      #size-cells = <1>;
      ranges;

      linux,cma {
          compatible = "shared-dma-pool";
          reusable;
          size = <0x14000000>;
          linux,cma-default;
      };
  };

  /* LED Configuration */
  leds {
      compatible = "gpio-leds";
      pinctrl-names = "default";
      pinctrl-0 = <&pinctrl_led>;

      user_led {
          label = "Heartbeat-Main";
          gpios = <&gpio2 3 0>;
          linux,default-trigger = "heartbeat";
	    };

      wifi_led {
        label = "Wifi-Access";
        gpios = <&gpio2 05 0>;
        linux,default-trigger = "mmc0";
      };          
  };

  /* Input button configuration */
  gpio_keys {
      compatible = "gpio-keys";
      pinctrl-names = "default";
      pinctrl-0 = <&pinctrl_gpiokeys>;
      #address-cells = <1>;
      #size-cells = <0>;
      autorepeat;

      swiftcore-btn {
          label = "Swiftcore-Input";
          linux,code = <28>;
          gpios = <&gpio1 31 GPIO_ACTIVE_LOW>;
      };
  };

  /* Reset Line Configuration */
  gpio_resets {
    compatible = "linux,gpio-reset";
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_gpioreset>;
    #address-cells = <1>;
    #size-cells = <0>;
    asserted-state = <0>;
    duration-ms = <100>;
    auto;    

    /* GPS RESET_N */
    gnss {
      label = "GPS-Reset";
      gpios = <&gpio1 4 0>;
    };

    /* Cellular RESET_N */
    cell_reset {
      label = "Cell-Reset";
      gpios = <&gpio1 16 0>;
    };

    /* Sub 1 GHz RESET_N */
    sub1ghz {
      label = "Sub1-Reset";
      gpios = <&gpio4 3 0>;
    };

    /* Lora RESET_N */
    lora {
      label = "Lora-Reset";
      gpios = <&gpio5 1 0>;
    };

    /* Accelerometer RESET_N */
    accel {
      label = "Accel-Reset";
      gpios = <&gpio5 4 0>;
    };

    /* Camera RESET_N */
    camera {
      label = "Camera-Reset";
      gpios = <&gpio5 6 0>;
    };

    /* Display RESET_N */
    display {
      label = "Display-Reset";
      gpios = <&gpio5 8 0>;
    };

    /* USB Debug RESET_N */
    usbdebug {
      label = "USB-Reset";
      gpios = <&gpio5 9 0>;
    };

    /* Expander RESET_N */
    external {
      label = "Exp-Reset";
      gpios = <&gpio5 2 0>;
    };
  };


  regulators {
    compatible = "simple-bus";
    #address-cells = <1>;
    #size-cells = <0>;

    reg_can_3v3:
    regulator@0 {
        compatible = "regulator-fixed";
        reg = <0>;
        regulator-name = "can-3v3";
        regulator-min-microvolt = <3300000>;
        regulator-max-microvolt = <3300000>;
    };
	
		wlan_en_reg: 
		regulator@2 {
		compatible = "regulator-fixed";
		regulator-name = "wlan-en-regulator";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		startup-delay-us = <70000>;

		gpio = <&gpio4 9 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	  };

    reg_gpio_dvfs:
    regulator-gpio {
        compatible = "regulator-gpio";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_dvfs>;
        regulator-min-microvolt = <1300000>;
        regulator-max-microvolt = <1400000>;
        regulator-name = "gpio_dvfs";
        regulator-type = "voltage";
        gpios = <&gpio5 3 GPIO_ACTIVE_HIGH>;
        states = <1300000 0x1 1400000 0x0>;
    };
  };

  kim {
        compatible = "kim";
  	nshutdown_gpio = <106>;
        dev_name = "/dev/ttymxc2";
        flow_cntrl = <1>;
        baud_rate = <3000000>;
    };

  btwilink {
        compatible = "btwilink";
    };

};

&iomuxc {
  pinctrl-names = "default";

  pinctrl-0 = <&pinctrl_hog_1>;
  imx6ul-evk {

    pinctrl_hog_1: hoggrp-1 {
      fsl,pins = <
        MX6UL_PAD_ENET2_TX_EN__GPIO2_IO13  0x000010B0 /* Generic GPIO */
      >;
    };  
	
    /* wl12xx/wl18xx card on mmc3 */
  	usdhc1_pins_default: 
    pinmux_usdhc1_pins_default {
    	fsl,pins = <
    		MX6UL_PAD_SD1_CMD__USDHC1_CMD            0x000010B0
    		MX6UL_PAD_SD1_CLK__USDHC1_CLK            0x000010B0
    		MX6UL_PAD_SD1_DATA0__USDHC1_DATA0        0x000010B0
    		MX6UL_PAD_SD1_DATA1__USDHC1_DATA1        0x000010B0
    		MX6UL_PAD_SD1_DATA2__USDHC1_DATA2        0x000010B0
    		MX6UL_PAD_SD1_DATA3__USDHC1_DATA3        0x000010B0
    		>;
  	};

  	
  	/* wl12xx/wl18xx card enable/irq GPIOs. */
    wlan_pins_default: 
    pinmux_wlan_pins_default {
    	fsl,pins = <
    		MX6UL_PAD_ENET1_TX_DATA1__GPIO2_IO04     0x000010B0   /* wlan_iqr */
    		MX6UL_PAD_NAND_DATA07__GPIO4_IO09        0x000010B0 	/* wlan_en*/
        >;
    };


    pinctrl_led: 
    ledgrp {
      fsl,pins = <
      MX6UL_PAD_ENET1_TX_DATA0__GPIO2_IO03     0x000010B0  /* LED D301 */
      MX6UL_PAD_ENET1_TX_EN__GPIO2_IO05        0x000010B0  /* LED D302 */
      >;
    };

    pinctrl_gpiokeys:
    gpiokeygrp {
      fsl,pins = <
      MX6UL_PAD_UART5_RX_DATA__GPIO1_IO31      0x000010B0  /* USER Push Button */
      >;
    };

    /* Reset Line Group */
    pinctrl_gpioreset:
    gpiorstgrp {
      fsl,pins = <
      MX6UL_PAD_GPIO1_IO04__GPIO1_IO04         0x000010B0  /* GNSS RESET_N */
      MX6UL_PAD_UART1_TX_DATA__GPIO1_IO16      0x000010B0  /* Cellular RESET_N */
      MX6UL_PAD_NAND_DATA01__GPIO4_IO03        0x000010B0  /* Sub 1GHz RESET_N */
      MX6UL_PAD_SNVS_TAMPER1__GPIO5_IO01       0x000010B0  /* GNSS RESET_N */
      MX6UL_PAD_SNVS_TAMPER4__GPIO5_IO04       0x000010B0  /* Accelerometer RESET_N */
      MX6UL_PAD_SNVS_TAMPER6__GPIO5_IO06       0x000010B0  /* Camera RESET_N */
      MX6UL_PAD_SNVS_TAMPER8__GPIO5_IO08       0x000010B0  /* Display RESET_N */
      MX6UL_PAD_SNVS_TAMPER9__GPIO5_IO09       0x000010B0  /* USB Debug RESET_N */
      MX6UL_PAD_SNVS_TAMPER2__GPIO5_IO02       0x000010B0  /* GPIO Expander */
      >;
    };

    pinctrl_csi1:
    csi1grp {
      fsl,pins = <
      MX6UL_PAD_CSI_MCLK__CSI_MCLK		  0x1b088
      MX6UL_PAD_CSI_PIXCLK__CSI_PIXCLK	0x1b088
      MX6UL_PAD_CSI_VSYNC__CSI_VSYNC		0x1b088
      MX6UL_PAD_CSI_HSYNC__CSI_HSYNC		0x1b088
      MX6UL_PAD_CSI_DATA00__CSI_DATA02	0x1b088
      MX6UL_PAD_CSI_DATA01__CSI_DATA03	0x1b088
      MX6UL_PAD_CSI_DATA02__CSI_DATA04	0x1b088
      MX6UL_PAD_CSI_DATA03__CSI_DATA05	0x1b088
      MX6UL_PAD_CSI_DATA04__CSI_DATA06	0x1b088
      MX6UL_PAD_CSI_DATA05__CSI_DATA07	0x1b088
      MX6UL_PAD_CSI_DATA06__CSI_DATA08	0x1b088
      MX6UL_PAD_CSI_DATA07__CSI_DATA09	0x1b088
      >;
    };

    pinctrl_dvfs:
    dvfsgrp {
      fsl,pins = <
      MX6UL_PAD_SNVS_TAMPER3__GPIO5_IO03	0x79
      >;
    };

    pinctrl_flexcan2:
    flexcan2grp{
      fsl,pins = <
      MX6UL_PAD_UART2_RTS_B__FLEXCAN2_RX	0x1b020
      MX6UL_PAD_UART2_CTS_B__FLEXCAN2_TX	0x1b020
      >;
    };

    pinctrl_i2c3:
    i2c3grp {
      fsl,pins = <
      MX6UL_PAD_ENET2_RX_DATA0__I2C3_SCL 0x4001b8b0
      MX6UL_PAD_ENET2_RX_DATA1__I2C3_SDA 0x4001b8b0
      >;
    };

    pinctrl_lcdif_dat:
    lcdifdatgrp {
      fsl,pins = <
      MX6UL_PAD_LCD_DATA00__LCDIF_DATA00  0x79
      MX6UL_PAD_LCD_DATA01__LCDIF_DATA01  0x79
      MX6UL_PAD_LCD_DATA02__LCDIF_DATA02  0x79
      MX6UL_PAD_LCD_DATA03__LCDIF_DATA03  0x79
      MX6UL_PAD_LCD_DATA04__LCDIF_DATA04  0x79
      MX6UL_PAD_LCD_DATA05__LCDIF_DATA05  0x79
      MX6UL_PAD_LCD_DATA06__LCDIF_DATA06  0x79
      MX6UL_PAD_LCD_DATA07__LCDIF_DATA07  0x79
      MX6UL_PAD_LCD_DATA08__LCDIF_DATA08  0x79
      MX6UL_PAD_LCD_DATA09__LCDIF_DATA09  0x79
      MX6UL_PAD_LCD_DATA10__LCDIF_DATA10  0x79
      MX6UL_PAD_LCD_DATA11__LCDIF_DATA11  0x79
      MX6UL_PAD_LCD_DATA12__LCDIF_DATA12  0x79
      MX6UL_PAD_LCD_DATA13__LCDIF_DATA13  0x79
      MX6UL_PAD_LCD_DATA14__LCDIF_DATA14  0x79
      MX6UL_PAD_LCD_DATA15__LCDIF_DATA15  0x79
      MX6UL_PAD_LCD_DATA16__LCDIF_DATA16  0x79
      MX6UL_PAD_LCD_DATA17__LCDIF_DATA17  0x79
      MX6UL_PAD_LCD_DATA18__LCDIF_DATA18  0x79
      MX6UL_PAD_LCD_DATA19__LCDIF_DATA19  0x79
      MX6UL_PAD_LCD_DATA20__LCDIF_DATA20  0x79
      MX6UL_PAD_LCD_DATA21__LCDIF_DATA21  0x79
      MX6UL_PAD_LCD_DATA22__LCDIF_DATA22  0x79
      MX6UL_PAD_LCD_DATA23__LCDIF_DATA23  0x79
      >;
    };

    pinctrl_pwm1:
    pwm1grp {
        fsl,pins = <
        MX6UL_PAD_GPIO1_IO08__PWM1_OUT   0x110b0
        >;
    };

    pinctrl_qspi:
    qspigrp {
        fsl,pins = <
        MX6UL_PAD_NAND_WP_B__QSPI_A_SCLK      0x70a1
        MX6UL_PAD_NAND_READY_B__QSPI_A_DATA00 0x70a1
        MX6UL_PAD_NAND_CE0_B__QSPI_A_DATA01   0x70a1
        MX6UL_PAD_NAND_CE1_B__QSPI_A_DATA02   0x70a1
        MX6UL_PAD_NAND_CLE__QSPI_A_DATA03     0x70a1
        MX6UL_PAD_NAND_DQS__QSPI_A_SS0_B      0x70a1
        >;
    };

    pinctrl_sim2_1:
    sim2grp-1 {
        fsl,pins = <
        MX6UL_PAD_CSI_DATA03__SIM2_PORT1_PD		0xb808
        MX6UL_PAD_CSI_DATA04__SIM2_PORT1_CLK		0x11
        MX6UL_PAD_CSI_DATA05__SIM2_PORT1_RST_B		0xb810
        MX6UL_PAD_CSI_DATA06__SIM2_PORT1_SVEN		0xb810
        MX6UL_PAD_CSI_DATA07__SIM2_PORT1_TRXD		0xb811
        MX6UL_PAD_CSI_DATA02__GPIO4_IO23		0x3008
        >;
    };

    pinctrl_uart2:
    uart2grp {
        fsl,pins = <
        MX6UL_PAD_NAND_DATA04__UART2_DCE_TX	        0x1b0b1
        MX6UL_PAD_NAND_DATA05__UART2_DCE_RX	        0x1b0b1
        >;
    };

    pinctrl_uart3:
    uart3grp {
        fsl,pins = <
        MX6UL_PAD_UART3_RX_DATA__UART3_DCE_RX
        MX6UL_PAD_UART3_TX_DATA__UART3_DCE_TX
        MX6UL_PAD_UART3_CTS_B__UART3_DCE_CTS
        MX6UL_PAD_UART3_RTS_B__UART3_DCE_RTS
        >;
    };

  	pinctrl_uart5:
        uart5grp {
            fsl,pins = <
            MX6UL_PAD_UART5_TX_DATA__UART5_DCE_TX         0x000010B0
            MX6UL_PAD_GPIO1_IO05__UART5_DCE_RX            0x000010B0
            >;
        };

    pinctrl_uart7:
    uart7grp {
        fsl,pins = <
        MX6UL_PAD_ENET2_RX_EN__UART7_DCE_TX         0x000010B0
        MX6UL_PAD_ENET2_TX_DATA0__UART7_DCE_RX      0x000010B0
        >;
    };

    pinctrl_uart2dte:
    uart2dtegrp {
        fsl,pins = <
        MX6UL_PAD_UART2_TX_DATA__UART2_DTE_RX	      0x1b0b1
        MX6UL_PAD_UART2_RX_DATA__UART2_DTE_TX	      0x1b0b1

        >;
    };

    pinctrl_wdog:
    wdoggrp {
        fsl,pins = <
        MX6UL_PAD_LCD_RESET__WDOG1_WDOG_ANY    0x30b0
        >;
    };
  };
};

&cpu0 {
    arm-supply = <&reg_arm>;
    soc-supply = <&reg_soc>;
    dc-supply = <&reg_gpio_dvfs>;
};

&clks {
    assigned-clocks = <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>;
    assigned-clock-rates = <786432000>;
};

&flexcan2 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_flexcan2>;
    xceiver-supply = <&reg_can_3v3>;
    status = "okay";
};

&gpc {
    fsl,cpu_pupscr_sw2iso = <0x1>;
    fsl,cpu_pupscr_sw = <0x0>;
    fsl,cpu_pdnscr_iso2sw = <0x1>;
    fsl,cpu_pdnscr_iso = <0x1>;
    fsl,ldo-bypass = <0>; /* DCDC, ldo-enable */
};

&i2c3 {
    clock_frequency = <100000>;
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_i2c3>;
    status = "okay";

    si7050@40 {
        compatible = "si7050";
        reg = <0x40>;
		status = "okay";
    };

    pf3001@08 {
        compatible = "pf3001";
        reg = <0x08>;
		status = "okay";
    };

    icm30630@6b {
        compatible = "icm30630";
        reg = <0x6b>;
		status = "okay";
    };
};

